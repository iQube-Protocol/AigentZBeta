# Aigent Z Development Environment

# Next.js Environment Variables
NODE_ENV=development

# API URLs - All pointing to Flask backend on port 5000
NEXT_PUBLIC_CORE_API_URL=http://localhost:5000
NEXT_PUBLIC_REGISTRY_API_URL=http://localhost:5000
NEXT_PUBLIC_AIGENT_API_URL=http://localhost:5000

# Blockchain Configuration
NEXT_PUBLIC_WEB3_PROVIDER_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
NEXT_PUBLIC_DEFAULT_NETWORK=ethereum

# iQube Registry Configuration
NEXT_PUBLIC_IQUBE_REGISTRY_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890
NEXT_PUBLIC_BLAKQUBE_STORAGE_ENDPOINT=https://ipfs.io/ipfs/

# Security Settings
NEXT_PUBLIC_TOKEN_EXPIRATION_HOURS=24

# Development Flags
NEXT_PUBLIC_DEBUG=false
NEXT_PUBLIC_TESTING=false

# Analytics
NEXT_PUBLIC_ANALYTICS_ID=development
