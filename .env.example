# QubeAgent Configuration Example

# Blockchain Configuration
WEB3_PROVIDER_URL=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID
WALLET_PRIVATE_KEY=your_wallet_private_key_here
DEFAULT_NETWORK=ethereum

# AI Model Configurations
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Vector Database Configuration
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment
PINECONE_INDEX_NAME=qubeagent_index

# Logging and Monitoring
LOG_LEVEL=INFO
SENTRY_DSN=your_sentry_dsn_optional

# iQube Registry Configuration
IQUBE_REGISTRY_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890
BLAKQUBE_STORAGE_ENDPOINT=https://ipfs.io/ipfs/

# Security Settings
ENCRYPTION_KEY=generate_a_secure_random_key_here
TOKEN_EXPIRATION_HOURS=24

# Development Flags
DEBUG=False
TESTING=False

# Optional External Service Integrations
TAVILY_API_KEY=your_tavily_api_key_optional
